<table class="table">
    <thead>
      <tr>
        <th> Name </th>
        <th> Email </th>
        <th> Phone </th>
        <th> Contact Method </th>
        <th> Methods </th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| if user.id!=1 %>
        <tr>
          <td> <%= user.first_name + ' ' + user.last_name %></td>
          <td> <%= user.email %></td>
          <td> <%= user.phone %></td>
          <td> <%= user.contact_method %></td>
          <td><%= link_to 'Edit', {:controller => :users, :action => :edit, :id => user.id } %></td>
          <td> <%= link_to "Destroy", admin_destroy_user_path(user), method: :delete, data: { confirm: "You sure?" } %></td>

        </tr>
      <% end %>
    <% end %>
    </tbody>
  </table>