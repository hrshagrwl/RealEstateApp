<%= render "partials/navbar" %>
<div class="container" style="margin-top:50px">
  <div class="card">
    <div class="card-header">
      <h3>New User</h3>
    </div>
    <div class="card-body">
      <%= form_for @user, url: { :action => "update" } do |form| %>
        <%= render 'form', { f: form , user: @user} %>
      <% end %>
    </div>
  </div>
</div>